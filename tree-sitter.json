{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ucode",
      "camelcase": "Ucode",
      "title": "Ucode",
      "scope": "source.ucode",
      "file-types": [
        "ucode", "uc", "ut"
      ],
      "injection-regex": "^ucode$",
      "class-name": "TreeSitterUcode",
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ],
  "metadata": {
    "version": "0.0.20250306",
    "license": "MIT",
    "description": "Parser for the ucode language",
    "authors": [
      {
        "name": "Jo-Philipp Wich",
        "email": "jo@mein.io",
        "url": "https://mein.io/"
      }
    ],
    "links": {
      "repository": "https://github.com/jow-/tree-sitter-ucode"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
